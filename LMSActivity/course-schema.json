        "fired_at": {
          "type": "string"
        },
        "user_id": {
          "type": "integer"
        },
        "course_id": {
          "type": "integer"
        },
        "completion_date": {
          "type": "string"
        },
        "subscribed_by_id": {
          "type": "integer"
        },
        "status": {
          "type": "string"
        },
        "level": {
          "type": "string"
        },
        "enrollment_date": {
          "type": "string"
        },
        "enrollment_date_begin_validity": {
          "type": "null"
        },
        "enrollment_date_end_validity": {
          "type": "null"
        }
      },
      "required": [
        "fired_at",
        "user_id",
        "course_id",
        "completion_date",
        "subscribed_by_id",
        "status",
        "level",
        "enrollment_date",
